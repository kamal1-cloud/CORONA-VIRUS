  "devDependencies": {
    "autoprefixer": "^9.7.5",
    "build": "^0.1.4",
    "concat": "^1.0.3",
    "gulp-concat": "^2.6.1",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0"
  }
