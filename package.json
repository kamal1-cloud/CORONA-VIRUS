  "description": "C'est un projet pour valider les comp√©tences C3N3 (C2N3).",
  "main": "index.js",
  "scripts": {
    "compile": "node-sass Starter-file/sass -o Starter-file/dist/css",
    "watch": "node-sass -w Starter-file/sass -o Starter-file/dist/css",
    "concat": "concat -o Starter/dist/css/concat.css Starter/dist/css/main.css Starter/dist/css/fichier.css Starter/dist/css/additional.css",
    "prefix": "postcss Starter/dist/css/style.concat.css --use=autoprefixer --map=false --output=Starter/dist/css/style.prefix.css",
    "compress": "node-sass Starter/dist/css/style.prefix.css Starter/dist/css/style.css --output-style compressed",
    "build-css": "npm-run-all compile concat prefix compress"
  },
